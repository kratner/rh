{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "isolatedModules": true,
      "moduleResolution": "node",
      "paths": {
        "styled-components": ["./node_modules/styled-components"]
      }
    },
    "include": ["src"]
  }
  